{
  "name": "focusd",
  "description": "A markup framework built for expressing interpretation",
  "version": "0.1.1",
  "author": "jk",
  "dependencies": {
    "marked": "1.1.0",
    "moment-timezone": "0.5.31"
  },
  "devDependencies": {
    "css-loader": "3.6.0",
    "coffeescript": "2.5.1",
    "eslint": "7.3.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vuln-regex-detector": "1.0.4",
    "html-webpack-plugin": "4.3.0",
    "less": "3.11.3",
    "less-loader": "6.1.3",
    "screamjs": "0.8.3",
    "style-loader": "1.2.1",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "amd": true,
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2015
    },
    "plugins": [
      "standard"
    ],
    "rules": {
      "comma-spacing": "off",
      "curly": "off",
      "indent": [
        "warn",
        2,
        {
          "VariableDeclarator": {
            "var": 2
          },
          "SwitchCase": 1,
          "outerIIFEBody": 0
        }
      ],
      "key-spacing": "off",
      "no-cond-assign": "off",
      "no-control-regex": "off",
      "no-multi-spaces": "off",
      "no-return-assign": "off",
      "no-useless-escape": "off",
      "object-curly-spacing": "off",
      "one-var": "off",
      "operator-linebreak": [
        "error",
        "before",
        {
          "overrides": {
            "=": "after"
          }
        }
      ],
      "semi": "off",
      "space-before-function-paren": "off",
      "space-infix-ops": "off",
      "standard/object-curly-even-spacing": "off"
    }
  },
  "eslintIgnore": [
    "_/**.js"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jkresner/focusd.git"
  },
  "scripts": {
    "lint": "npx eslint lib/*.js",
    "test": "npx coffee test/index.coffee"
  }
}
