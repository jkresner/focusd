{
  "author": { "name": "J Kresner" },
  "bundleDependencies": false,
  "deprecated": false,
  "description": "A markup framework built for expressing interpretation",
  "dependencies": {
    "marked": "*",
    "moment": "*"
  },
  "devDependencies": {
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "2.0.2",
    "eslint": "4.15.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.0",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vuln-regex-detector": "1.0.4",
    "file-loader": "3.0.1",
    "html-differ": "1.3.4",
    "html-webpack-plugin": "3.2.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "screamjs": "0.8.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14"
  },
  "engines": {
    "node": ">=11.4.0"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": { "amd": true, "browser": true, "node": true },
    "parserOptions": { "ecmaVersion": 5 },
    "plugins": ["standard"],
    "rules": {
      "comma-spacing": "off",
      "curly": "off",
      "indent": ["warn", 2, { "VariableDeclarator": { "var": 2 }, "SwitchCase": 1, "outerIIFEBody": 0 }],
      "key-spacing": "off",
      "no-cond-assign": "off",
      "no-control-regex": "off",
      "no-multi-spaces": "off",
      "no-return-assign": "off",
      "no-useless-escape": "off",
      "object-curly-spacing": "off",
      "one-var": "off",
      "operator-linebreak": ["error", "before", { "overrides": { "=": "after" } } ],
      "semi": "off",
      "space-before-function-paren": "off",
      "space-infix-ops": "off",
      "standard/object-curly-even-spacing": "off"
    }
  },
  "eslintIgnore": [
    "_/**.js"
  ],
  "license": "MIT",
  "name": "focusd",
  "repository": {
    "type": "git",
    "url": "git://github.com/jkresner/focusd.git"
  },
  "scripts": {
    "dev": "npx webpack-dev-server --config demo/wpk.js",
    "lint": "npx eslint lib/*.js",
    "test": "npx coffee test/index.coffee"
  },
  "tags": [
    "markdown",
    "markup",
    "html"
  ],
  "version": "0.0.3"
}
