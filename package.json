{
  "name": "focusd",
  "description": "A markup framework built for expressing interpretation",
  "version": "0.1.0",
  "author": "jk",
  "dependencies": {
    "marked": "0.5.0",
    "moment-timezone": "0.5.21"
  },
  "devDependencies": {
    "css-loader": "2.1.0",
    "eslint": "5.0.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vuln-regex-detector": "1.0.4",
    "html-webpack-plugin": "3.2.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "screamjs": "0.8.2",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.14"
  },
  "eslintConfig": {
    "extends": "standard",
    "env": { "amd": true, "browser": true, "node": true },
    "parserOptions": { "ecmaVersion": 5 },
    "plugins": ["standard"],
    "rules": {
      "comma-spacing": "off",
      "curly": "off",
      "indent": ["warn", 2, { "VariableDeclarator": { "var": 2 }, "SwitchCase": 1, "outerIIFEBody": 0 }],
      "key-spacing": "off",
      "no-cond-assign": "off",
      "no-control-regex": "off",
      "no-multi-spaces": "off",
      "no-return-assign": "off",
      "no-useless-escape": "off",
      "object-curly-spacing": "off",
      "one-var": "off",
      "operator-linebreak": ["error", "before", { "overrides": { "=": "after" } } ],
      "semi": "off",
      "space-before-function-paren": "off",
      "space-infix-ops": "off",
      "standard/object-curly-even-spacing": "off"
    }
  },
  "eslintIgnore": [
    "_/**.js"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/jkresner/focusd.git"
  },
  "scripts": {
    "lint": "npx eslint lib/*.js",
    "test": "SCREAM"
  }
}
